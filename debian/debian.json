{
  "builders": [
    {
      "type": "qemu",
      "iso_url": "{{user `url`}}",
      "iso_checksum": "{{user `iso_checksum`}}",
      "shutdown_command": "echo 'packer' | sudo -S shutdown -P now",
      "disk_size": "5000M",
      "format": "qcow2",
      "memory": "1024",
      "http_directory": "./",
      "ssh_username": "root",
      "ssh_password": "P@ssw0rd",
      "ssh_timeout": "20m",
      "vm_name": "debian.qcow2",
      "net_device": "virtio-net",
      "disk_interface": "virtio",
      "qemuargs": [
      ["-m", "1024"],
      ["-smp", "4"]
        ],
      "boot_wait": "10s",
      "boot_command": [
        "<esc><wait>",
        "auto ",
        "net.ifnames=0 ",
        "preseed/url=http://{{.HTTPIP}}:{{.HTTPPort}}/preseed.cfg ",
        "<enter>"
      ]
    }],
  "provisioners": [{
    "type": "shell",
    "scripts": [
      "zabbix.sh"
    ]
  }],  
  "variables": {
    "iso_checksum": "b462643a7a1b51222cd4a569dad6051f897e815d10aa7e42b68adc8d340932d861744b5ea14794daa5cc0ccfa48c51d248eda63f150f8845e8055d0a5d7e58e6",
